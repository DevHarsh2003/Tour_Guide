<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Display Data</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>

<style>
    body {
    font-family: Arial, sans-serif;
    margin: 20px;
}


    #main{
        position: absolute;
        top: 7rem;
        left: 25rem;

      background-color: lightgray;
      width:30rem;
      margin-top: 1rem;
      padding: 2rem;
      border-radius: 0.7rem;
    }
    #form-details{
      margin-top: 2rem;
    }
    #name{
    font-size: 2rem;
  }
  #name-div{
    margin-top: 8rem;
    text-align: center;
  }
  .btn{
    background-color: rgb(49, 193, 1);
    color: aliceblue;
    font-size: 0.9rem;
    margin-left:0rem;
    margin-right:1.2rem;
    margin-top: 0.9rem;
    width:12rem;
  } 
  #nav-btn{
    background-color: rgb(49, 193, 1);
    color: aliceblue;
    font-size:0.9rem;
    margin-left:0rem;
    margin-right:0rem;
  }
  #buttons-div{
    text-align: center;
  }
  hr{
    height:0.03rem;  
    background-color: lightgray;
    width:70rem;
  }
h2 {
    margin-bottom: 10px;
}

table {
    width: 100%;
    border-collapse: collapse;
    margin-bottom: 20px;
}

table, th, td {
    border: 1px solid #ddd;
}

th, td {
    padding: 12px;
    text-align: left;
}

th {
    background-color: #f2f2f2;
}

</style>
<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
       
        <a class="navbar-brand" href="#"> 
          <img src="../images/IMG-20240102-WA0001.jpg" alt="" height="50rem" width="60rem">
      </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse justify-content-end" id="navbarNav">
          <ul class="navbar-nav">
            <li class="nav-item" >
              <a  class="nav-link btn" id="nav-btn" href="/admin">Home</a>
            </li>&nbsp;&nbsp;
           
          </ul>
        </div>
      </nav>

    <h2>Featured Places</h2>
    <table>
        <tr>
            <th>City</th>
            <th>Name</th>
            <th>Address</th>
        </tr>
        <% data.forEach(item => { %>
            <tr>
                <td><%= item.city %></td>
                <td><%= item.places %></td>
                <td><%= item.addfp %></td>
            </tr>
        <% }); %>
    </table>

    <h2>Restobars</h2>
    <table>
        <tr>
            <th>City</th>
            <th>Name</th>
            <th>Address</th>
        </tr>
        <% data2.forEach(item => { %>
            <tr>
                <td><%= item.city %></td>
                <td><%= item.rest %></td>
                <td><%= item.addrb %></td>
            </tr>
        <% }); %>
    </table>

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.1/dist/umd/popper-base.min.js"
            integrity="sha384-rJlklvTlT+DW2l1zrHrFOq49r4giO+zEyQJ1mXkF3Gs62Z/tAe+wNOJ7Vj+R1Cjr"
            crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</body>
</html>
